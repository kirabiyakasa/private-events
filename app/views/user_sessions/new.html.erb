<div class="new-post-container">
  <%= form_with scope: :session, url: user_sessions_path  do |f| %>
    <%= f.label :username, 'Username:', class: "form-label" %>
    <%= f.text_field :username, class: 'text-field' %>
    <%= f.submit 'Sign In', class: "submit-button" %>
  <% end %>
  <%= link_to '<<< Go Back', events_path, class: "go-back" %>
</div>